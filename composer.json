{
  "name": "gimmenetwork/simplon_components",
  "description": "Allows installation of Components via Composer.",
  "type": "composer-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Rob Loach",
      "homepage": "http://robloach.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "ComponentInstaller\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "class": "ComponentInstaller\\ComponentInstallerPlugin"
  },
  "require": {
    "php": ">=5.4",
    "kriswallsmith/assetic": "1.*",
    "composer-plugin-api": "^2.0"
  },
  "require-dev": {
    "composer/composer": "2.0.*",
    "phpunit/phpunit": "4.*"
  },
  "replace": {
    "simplon/components": "1.0.x-dev"
  }
}
